  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponenten-vl-ui-bom.git"
  },
  "homepage": "https://github.com/milieuinfo/webcomponenten-vl-ui-bom#readme",
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "scripts": {
    "generate": "npm run generate:copy && npm run generate:replace",
    "generate:copy": "npm run generate:copy:webcomponent && npm run generate:copy:test && npm run generate:copy:demo && npm run generate:copy:style",
    "generate:copy:webcomponent": "copyfiles -f -e '**/*.min.js' -e '**/*.src.js' node_modules/*/dist/vl-*.js .",
    "generate:copy:test":  "npm run generate:copy:test:tests && npm run generate:copy:test:components && npm run generate:copy:test:pages",
    "generate:copy:test:tests":  "copyfiles -f node_modules/vl-*/test/e2e/* ./test",
    "generate:copy:test:components":  "copyfiles -f node_modules/vl-*/test/e2e/components/* ./test/components",
    "generate:copy:test:pages":  "copyfiles -f node_modules/vl-*/test/e2e/pages/* ./test/pages",
    "generate:copy:demo": "npm run generate:copy:demo:files && npm run generate:copy:demo:directory:img",
    "generate:copy:demo:files": "copyfiles -f node_modules/vl-*/demo/* ./demo",
    "generate:copy:demo:directory:img": "copyfiles -f node_modules/vl-*/demo/img/* ./demo/img",
    "generate:copy:style": "node ./scripts/copy-styles.js",
    "generate:replace": "replace '/src/(.*).js' '/$1.js' ./demo/vl*.*",
    "release:prepare": "echo no release prepare necessary",
    "release:testless": "np --yolo",
    "test:server:start": "http-server &",
    "test": "npm run test:server:start && mocha --chrome && mocha --firefox",
    "test:grid": "npm run test:server:start && mocha --chrome --browserstack --bom",
    "posttest": "pkill -f http-server"
  },
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponenten-vl-ui-bom/issues"
  },
